/**app.wxss**/
page {
  background: #f3f3f3;
  /* display: flex;
  flex-direction: column;
  justify-content: flex-start; */
}
.shade {
  min-width: 100vw;
  min-height: 100vh;
  background-color: rgb(0, 0, 0);
  opacity: 0.2;
  z-index: 1;
  position: fixed;
}
/* 图标 */
.icon-xxs{
  width: 50rpx;
  height: 50rpx;
}
.icon-xs{
  width: 55rpx;
  height: 55rpx;
}
.icon-s{
  width: 60rpx;
  height: 60rpx;
}
.icon-m{
  width: 65rpx;
  height: 65rpx;
}
.icon-l{
  width: 70rpx;
  height: 70rpx;
}
/* 文字 */
.text-xs{
  font-size: 23rpx;
  color: grey;
}
.text-s{
  font-size: 26rpx;
  color: grey;
}
.text-m{
  font-size: 32rpx;
  color: grey;
  font-weight: bold;
}
.text-l{
  font-weight: bold;
}
/* 按钮 */
button {
  background: initial;
}
button:focus{
  outline: 0;
}
button::after{
  border: none;
}
